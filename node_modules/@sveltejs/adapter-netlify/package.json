{
  "name": "@sveltejs/adapter-netlify",
  "version": "5.2.4",
  "description": "A SvelteKit adapter that creates a Netlify app",
  "keywords": [
    "adapter",
    "deploy",
    "hosting",
    "netlify",
    "svelte",
    "sveltekit"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sveltejs/kit.git",
    "directory": "packages/adapter-netlify"
  },
  "license": "MIT",
  "homepage": "https://svelte.dev/docs/kit/adapter-netlify",
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "index.d.ts",
  "files": [
    "files",
    "index.js",
    "index.d.ts"
  ],
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "esbuild": "^0.25.4",
    "set-cookie-parser": "^2.6.0"
  },
  "devDependencies": {
    "@netlify/edge-functions": "^2.15.1",
    "@netlify/functions": "^4.0.0",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0-next.3",
    "@types/node": "^18.19.119",
    "@types/set-cookie-parser": "^2.4.7",
    "rollup": "^4.14.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4",
    "@sveltejs/kit": "^2.43.7"
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.4.0"
  },
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c",
    "check": "tsc",
    "lint": "prettier --check .",
    "format": "pnpm lint --write",
    "test": "pnpm test:unit && pnpm test:integration",
    "test:unit": "vitest run",
    "test:integration": "pnpm build && pnpm -r --workspace-concurrency 1 --filter=\"./test/**\" test"
  }
}